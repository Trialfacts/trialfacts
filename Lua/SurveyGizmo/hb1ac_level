logging = true

result = tonumber(getvalue(211))
passfail = 231

function log(var) 
  if logging == true then
    print(var)
    end
 end

log("start")
  
if result == nil then
  result = 7.5
  log("nil")
end

  
if (result > 6.5) and (result < 10) then
  setvalue(passfail, "Pass") 
  log("Pass")
else 
  setvalue(passfail, "Fail")
  log("Fail")
end
  
log("end")
